std::sqrt( std::pow( x-x_1, 2 ) + std::pow( y-y_1, 2 ) );
std::sqrt( std::pow( x-x_1, 2 ) + std::pow( y-y_1, 2 ) + std::pow( z-z_1, 2 ) );
